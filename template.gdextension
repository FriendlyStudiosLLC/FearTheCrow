[configuration]
entry_symbol = "entrypoint"
compatibility_minimum = 4.3
reloadable = true

[icons]
Player = "res://bin/${LIBRARY_NAME}/Icons/Player.svg"
InteractionComponent = "res://bin/${LIBRARY_NAME}/Icons/InteractionComponent.svg"

[libraries]
linux${DEBUG_POSTFIX}.x86_64 = "${LIBRARY_NAME}.so"
macos${DEBUG_POSTFIX} = "${LIBRARY_NAME}.dylib"
windows${DEBUG_POSTFIX}.x86_64 = "${LIBRARY_NAME}.dll"